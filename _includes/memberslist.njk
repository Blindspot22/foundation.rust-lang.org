<h2>Founding Platinum</h2>
<ul class="memberslist">
    {% for member in memberslist %}
        {% if member.data.level == "founding" %}
            <li class="platinummember">
                {% if member.data.logo %}
                    <img src="/img/member/{{ member.data.logo }}" alt="{{member.data.name}}"/>
                {% endif %}
            </li>
        {% endif %}
    {% endfor %}
</ul>
